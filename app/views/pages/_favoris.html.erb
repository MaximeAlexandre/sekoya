<% unless helpers.include?(booking.helper) %>
    <%= link_to "♡", favoris_path(booking: booking, path: path),
    method: :post, class: "btn seko-small-button m-2" %>
<% else  %>
    <%= link_to "♥", favoris_destroy_path(booking, path: path),
    method: :delete, class: "btn seko-small-button m-2" %>
<% end  %>
